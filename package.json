{
  "name": "vue3-openlayers",
  "version": "0.2.1",
  "description": "Openlayers Wrapper for Vue3",
  "repository": {
    "type": "git",
    "url": "https://github.com/MelihAltintas/vue3-openlayers"
  },
  "homepage": "https://github.com/MelihAltintas/vue3-openlayers",
  "bugs": {
    "url": "https://github.com/MelihAltintas/vue3-openlayers/issues"
  },
  "keywords": [
    "front-end",
    "web",
    "vue",
    "vuejs",
    "vue-js",
    "openlayers",
    "vue3-openlayers",
    "map",
    "gis",
    "vuelayers",
    "vue-openlayers"
  ],
  "author": "Melih Altıntaş",
  "license": "MIT",
  "private": false,
  "main": "./dist/vue3-openlayers.umd.js",
  "module": "./dist/vue3-openlayers.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-openlayers.es.js",
      "require": "./dist/vue3-openlayers.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/vue3-openlayers.css": {
      "import": "./dist/vue3-openlayers.css",
      "require": "./dist/vue3-openlayers.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "prepare-npm": "vite build && rm -rf ./npm && cp -r dist ./npm && cp package.json ./npm/ && cp README.md ./npm/ && cp LICENSE ./npm/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "index.d.ts",
  "dependencies": {
    "file-saver": "2.0.5",
    "jspdf": "2.5.1",
    "ol": "7.3.0",
    "ol-contextmenu": "5.2.0",
    "ol-ext": "4.0.6",
    "proj4": "2.9.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/node": "^18.15.9",
    "@types/ol-ext": "npm:@i209/types-ol-ext@^3.0.10",
    "@types/proj4": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@typescript-eslint/typescript-estree": "^5.55.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.9.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0",
    "vue": "3.2.47",
    "vitepress": "^1.0.0-alpha.62",
  }
}
